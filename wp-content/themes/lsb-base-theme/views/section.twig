<section class="lsb_section is-{{ section.lsb_layout }}">
	<div class="lsb_section__header">
		<h2 class="lsb_section__title">{{ section.lsb_title }} <span>{{ section.lsb_subtitle }}</span></h2>
	</div>
	<div class="lsb_section__body">
		{% for post in section.lsb_posts %}
			{% include ['tease-'~post.post_type~'.twig', 'tease.twig'] with {'h_size': 'h3'} %}
		{% endfor %}
	</div>
</section>
