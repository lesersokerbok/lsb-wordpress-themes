<article class="lsb_entry is-archive {{ description ? 'has-description' : ''}}">
	<div class="lsb_entry__header">
		<h1 class="lsb_entry__title">{{ title }}</h1>
	</div>
	{% if description %}
		<div class="lsb_entry__body is-content">
			{{ description }}
		</div>
	{% endif %}
	<section class="lsb_block">
		<div class="lsb_block__body">
			{% for post in posts %}
				{% include ['tease-'~post.post_type~'.twig', 'tease.twig'] %}
			{% endfor %}
		</div>
	</section>
</article>
